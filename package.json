{
  "name": "solid-start-template",
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "format": "yarn prettier . --write",
    "commit": "cz",
    "prepare": "husky"
  },
  "dependencies": {
    "@solidjs/meta": "0.29.3",
    "@solidjs/router": "0.13.3",
    "@solidjs/start": "1.0.0-rc.1",
    "solid-js": "1.8.17",
    "vinxi": "0.3.11"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@julr/vite-plugin-validate-env": "1.1.1",
    "@tailwindcss/vite": "4.0.0-alpha.14",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "tailwindcss": "4.0.0-alpha.14"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
